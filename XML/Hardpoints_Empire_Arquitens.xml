             <Fire_Cone_Width>40.0</Fire_Cone_Width>
             <Fire_Cone_Height>40.0</Fire_Cone_Height>
             <Allow_Opportunity_Fire_When_Targeting>True</Allow_Opportunity_Fire_When_Targeting>
             <Is_Turret> Yes </Is_Turret>
             <Turret_Rest_Angle> 0.0, 0.0, 0.0 </Turret_Rest_Angle>
             <Turret_Rotation_Offset> 0.0, 0.0, 0.0 </Turret_Rotation_Offset>
             <Turret_Rotate_Speed> 4.0 </Turret_Rotate_Speed>
             <Turret_Rotate_Extent_Degrees> 180.0 </Turret_Rotate_Extent_Degrees>
             <Turret_Elevate_Extent_Degrees> 45.0 </Turret_Elevate_Extent_Degrees>
             <Turret_Bone_Name> TURRET_ROTATION </Turret_Bone_Name>
             <Barrel_Bone_Name> TURRET_ELEVATION </Barrel_Bone_Name>


            <Fire_Projectile_Type>Proj_Ship_Light_Turbolaser_Green_Quad</Fire_Projectile_Type>


				 <!-- Reload Rate -->
             <Fire_Min_Recharge_Seconds>2.5</Fire_Min_Recharge_Seconds>
             <Fire_Max_Recharge_Seconds>2.5</Fire_Max_Recharge_Seconds>
			 <!-- Burst Rate -->
             <Fire_Pulse_Count>1</Fire_Pulse_Count>
			 <!-- Fire Rate -->
             <Fire_Pulse_Delay_Seconds>1.0</Fire_Pulse_Delay_Seconds>
             <Fire_Range_Distance>1400.0</Fire_Range_Distance>
