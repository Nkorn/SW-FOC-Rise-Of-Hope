            <Damage_Decal/>
            <Damage_Particles/>


			<Damage_Type> Damage_Acclamator </Damage_Type>


			 <Fire_Bone_A>MUZZLEA_00</Fire_Bone_A>
             <Fire_Bone_B>MUZZLEA_00</Fire_Bone_B>
